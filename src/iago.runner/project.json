{
    "name":"iago.runner",
    "version":"0.4.0-beta6",
    "description":"Iago runner is a tool to run bdd like tests in kre",
    "projectUrl":"https://github.com/rhwy/iago",
    "tags":["tdd", "bdd", "test", "dnx"],
    "dependencies":{
        "iago.lib":{"version": "0.4-*", "type": "build"},
        "iago.watcher":{"version": "1.0.0-*", "type": "build"},
        "iago.consoleWriter":{"version": "0.1", "type": "build"},
        "NFluent":"1.3.1.0",
        "Microsoft.Framework.Runtime.Abstractions": "1.0.0-beta6-*",
        "Microsoft.Framework.Configuration":"1.0.0-beta5-11396",
        "Microsoft.Framework.Configuration.CommandLine":"1.0.0-beta6-*"
    },
    
    "commands":{
        "test":"run",
        "mon":"iago.watcher --dnx=test"   
    },
    "frameworks": {
        "dnx451": {
            "frameworkAssemblies": {
                "System.Collections": "",
                "System.ObjectModel": "",
                "System.Reflection": "",
                "System.Runtime": "",
                "System.Text.RegularExpressions": "",
                "System.Xml": "",
                "System.Xml.Linq": ""
            }
        }
    }
}
